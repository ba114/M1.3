	<table type="2D" name="Injector Dead Time" category="Fuel" storagetype="uint8" endian="little" sizex="5" userlevel="1">
		<scaling units="ms" expression="x*0.010000" to_byte="x/0.010000" format="0.000" fineincrement="0.01" coarseincrement="0.1" />
